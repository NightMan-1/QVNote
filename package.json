{
    "name": "qvnote",
    "version": "0.5.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "@coreui/coreui": "^2.1.7",
        "css-loader": "^2.1.0",
        "cxlt-vue2-toastr": "^1.1.0",
        "echarts": "^4.2.0-rc.2",
        "glob-all": "^3.1.0",
        "lodash": "^4.17.11",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-url": "^8.0.0",
        "prismjs": "^1.15.0",
        "purgecss-webpack-plugin": "^1.4.0",
        "quill-image-drop-module": "^1.0.3",
        "quill-image-resize-module": "git+https://github.com/LeavittSoftware/leavitt-quill-image-resize-module.git",
        "simplebar-vue": "^0.1.4",
        "tingle.js": "^0.13.2",
        "v-charts": "^1.19.0",
        "vue": "^2.6.8",
        "vue-calendar-heatmap": "^0.8.4",
        "vue-headful": "^2.0.1",
        "vue-i18n": "^8.8.2",
        "vue-ls": "^3.2.1",
        "vue-multiselect": "^2.1.4",
        "vue-prism-editor": "^0.1.2",
        "vue-resource": "^1.5.1",
        "vue-router": "^3.0.2",
        "vue2-editor": "^2.6.6",
        "vuex": "^3.1.0"
    },
    "devDependencies": {
        "@fortawesome/fontawesome-free": "^5.7.2",
        "@vue/cli-plugin-babel": "^3.4.1",
        "@vue/cli-plugin-eslint": "^3.4.1",
        "@vue/cli-service": "^3.4.1",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.15.1",
        "eslint-plugin-vue": "^5.2.2",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.0.1",
        "vue-template-compiler": "^2.6.8"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "rules": {
            "no-tabs": 0,
            "no-mixed-spaces-and-tabs": 0,
            "new-cap": 0,
            "generator-star-spacing": "off",
            "vue/no-use-v-if-with-v-for": [
                "error",
                {
                    "allowUsingIterationVar": true
                }
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "postcss-import": {},
            "postcss-url": {},
            "autoprefixer": {},
            "cssnano": {
                "discardComments": {
                    "removeAll": true
                },
                "zindex": false
            }
        }
    },
    "browserslist": [
        "last 2 chrome versions",
        "last 2 firefox versions",
        "Firefox ESR",
        "last 2 safari versions",
        "last 2 opera versions",
        "last 2 ios_saf versions",
        "last 2 edge versions",
        "last 2 and_chr versions"
    ]
}
